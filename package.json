{
  "name": "syntax-parser",
  "version": "1.0.13",
  "pri": {
    "type": "component"
  },
  "scripts": {
    "start": "pri dev",
    "docs": "pri docs",
    "build": "pri build",
    "preview": "pri preview",
    "analyse": "pri analyse",
    "test": "pri test",
    "format": "tslint --fix './src/**/*.?(ts|tsx)' && prettier --write './src/**/*.?(ts|tsx)'",
    "prepublishOnly": "npm run build && npm run bundle",
    "bundle": "pri bundle",
    "release": "npm publish",
    "posttest": "codecov -f coverage/*.json -t dc6610f3-32ac-4d30-8676-74d3888349a0"
  },
  "devDependencies": {
    "pri": "^2.0.21"
  },
  "main": "dist/index.js",
  "types": "src/index.tsx",
  "peerDependencies": {},
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
